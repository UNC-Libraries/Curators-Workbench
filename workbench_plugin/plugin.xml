<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="unc.lib.cdr.workbench.accession.OriginalsPropertyTester"
            id="cdr-workbench.propertyTesterOriginals"
            namespace="cdr-workbench"
            properties="staged,captured"
            type="org.eclipse.core.resources.IResource">
      </propertyTester>
      <propertyTester
            class="unc.lib.cdr.workbench.xwalk.MetadataSourcePropertyTester"
            id="cdr-workbench.propertyTesterMetadataSource"
            namespace="cdr-workbench"
            properties="isTabularMetadataSource"
            type="org.eclipse.core.resources.IFile">
      </propertyTester>
      <propertyTester
            class="unc.lib.cdr.workbench.DivPropertyTester"
            id="cdr-workbench.propertyDivTester"
            namespace="cdr-workbench"
            properties="isContainer,isFolder"
            type="gov.loc.mets.DivType">
      </propertyTester>
      <propertyTester
            class="unc.lib.cdr.workbench.accession.PropertyTesterTopFolderName"
            id="workbench_plugin.testTopFolderName"
            namespace="workbench_plugin.testTopFolderName"
            properties="folderName"
            type="org.eclipse.core.resources.IResource">
      </propertyTester>
      <propertyTester
            class="unc.lib.cdr.workbench.tester.ProjectPropertyTester"
            id="workbench_plugin.projectPropertyTester"
            namespace="workbench_plugin.projectPropertyTester"
            properties="open,closed"
            type="org.eclipse.core.resources.IProject">
      </propertyTester>
   </extension>

   <extension
         id="application"
         name="Curator&apos;s Workbench Application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="unc.lib.cdr.workbench.rcp.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="unc.lib.cdr.workbench.commands.ExitHandler"
            id="unc.lib.cdr.pw.commands.Exit"
            name="Exit">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.commands.CreateProjectHandler"
            id="cdr-workbench.CreateProject"
            name="Create Project">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.arrange.RemoveDivHandler"
            id="cdr-workbench.RemoveDiv"
            name="Remove Div">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.accession.CaptureHandler"
            id="cdr-workbench.commandCaptureObject"
            name="Capture Object">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.accession.LinkOriginalsHandler"
            id="cdr-workbench.commandLinkOriginals"
            name="Link to Originals">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.xwalk.ImportMetadataSourceHandler"
            id="cdr-workbench.commandImportTabularMetadata"
            name="Crosswalk Batch Metadata">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.arrange.AddFolderDivHandler"
            id="cdr-workbench.AddFolderDiv"
            name="New Folder">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.arrange.RenameDivHandler"
            id="cdr-workbench.RenameDiv"
            name="Rename Div">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.commands.OpenCrosswalkHandler"
            id="cdr-workbench.commandOpenCrosswalk"
            name="Edit Crosswalk">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.describe.EditDescriptionCommand"
            id="cdr-workbench.commandEditDescription"
            name="Edit Description (MODS)">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.commands.ExportCDRSIPHandler"
            description="Export CDR Submission"
            id="cdr-workbench.commandExportCDRSubmission"
            name="Export CDR Submission">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.accession.RescanOriginalsHandler"
            description="Scan for changes to original files and folders"
            id="cdr-workbench.commandRescanOriginals"
            name="Refresh">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.commands.CloseProjectHandler"
            id="workbench_plugin.commandCloseProject"
            name="Close Project">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.commands.OpenProjectHandler"
            id="workbench_plugin.commandOpenProject"
            name="Open Project">
      </command>
      <command
            defaultHandler="unc.lib.cdr.workbench.commands.ConvertToCollectionHandler"
            description="Make this folder into a collection."
            id="workbench_plugin.commandConvertToCollection"
            name="Convert to Collection">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="fileMenu"
               label="Project">
            <command
                  commandId="cdr-workbench.CreateProject"
                  icon="icons/48px-Crystal_Clear_app_kthememgr.png"
                  label="Create Project"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.window.preferences"
                  label="Preferences"
                  style="push">
            </command>
            <command
                  commandId="unc.lib.cdr.pw.commands.Exit"
                  label="Exit"
                  style="push"
                  tooltip="Exit the workbench">
            </command>
            <command
                  commandId="org.eclipse.ui.project.closeProject"
                  style="push">
            </command>
         </menu>
         <menu
               id="help"
               label="Help">
            <command
                  commandId="org.eclipse.ui.help.aboutAction"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="true"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="cdr-workbench.RemoveDiv"
               label="Remove"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false">
                     <instanceof
                           value="gov.loc.mets.DivType">
                     </instanceof>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="cdr-workbench.commandCaptureObject"
               label="Capture"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false">
                     <and>
                        <test
                              property="workbench_plugin.testTopFolderName.folderName"
                              value="originals">
                        </test>
                        <or>
                           <instanceof
                                 value="org.eclipse.core.resources.IFolder">
                           </instanceof>
                           <instanceof
                                 value="org.eclipse.core.resources.IFile">
                           </instanceof>
                        </or>
                     </and>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="cdr-workbench.commandLinkOriginals"
               label="Link to Original Folders"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false">
                     <or>
                        <instanceof
                              value="org.eclipse.core.resources.IProject">
                        </instanceof>
                        <instanceof
                              value="unc.lib.cdr.workbench.accession.OriginalFoldersProjectElement">
                        </instanceof>
                     </or>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="cdr-workbench.commandImportTabularMetadata"
               label="Crosswalk Batch Metadata"
               mnemonic="T"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <and>
                  <with
                        variable="activeMenuSelection">
                     <count
                           value="1">
                     </count>
                  </with>
                  <with
                        variable="activeMenuSelection">
                     <iterate
                           operator="or">
                        <instanceof
                              value="org.eclipse.core.resources.IFile">
                        </instanceof>
                     </iterate>
                  </with>
               </and>
            </visibleWhen>
         </command>
         <command
               commandId="cdr-workbench.AddFolderDiv"
               label="Add Folder"
               mnemonic="F"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false">
                        
                  <or>
                     <instanceof
                           value="unc.lib.cdr.workbench.arrange.ArrangementProjectElement">
                     </instanceof>
                     <test
                           property="cdr-workbench.isContainer">
                     </test>
               </or>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="cdr-workbench.RenameDiv"
               label="Rename.."
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false">
                     <instanceof
                           value="gov.loc.mets.DivType">
                     </instanceof>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="cdr-workbench.commandOpenCrosswalk"
               label="Edit Crosswalk"
               style="push">
            <visibleWhen
                  checkEnabled="true">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false">
                     <instanceof
                           value="unc.lib.cdr.workbench.xwalk.CrosswalkTreeElement">
                     </instanceof>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="cdr-workbench.commandEditDescription"
               label="Edit Description"
               style="push">
            <visibleWhen
                  checkEnabled="true">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false">
                     <instanceof
                           value="gov.loc.mets.DivType">
                     </instanceof>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="cdr-workbench.commandExportCDRSubmission"
               label="Export CDR Submission"
               style="push"
               tooltip="Export a METS manifest for CDR ingest.">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <with
                        variable="activeMenuSelection">
                     <iterate
                           ifEmpty="false">
                        <instanceof
                              value="org.eclipse.core.resources.IProject">
                        </instanceof>
                     </iterate>
                  </with>
               </or>
            </visibleWhen>
         </command>
         <command
               commandId="cdr-workbench.commandRescanOriginals"
               label="Refresh"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false">
                     <or>
                        <instanceof
                              value="org.eclipse.core.resources.IProject">
                        </instanceof>
                        <instanceof
                              value="unc.lib.cdr.workbench.accession.OriginalFoldersProjectElement">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.core.resources.IResource">
                        </instanceof>
                     </or>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="workbench_plugin.commandCloseProject"
               label="Close Project"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false"
                        operator="or">
                     <test
                           property="workbench_plugin.projectPropertyTester.open">
                     </test>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="workbench_plugin.commandOpenProject"
               label="Open Project"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false"
                        operator="or">
                     <test
                           property="workbench_plugin.projectPropertyTester.closed">
                     </test>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="workbench_plugin.commandConvertToCollection"
               label="Convert to Collection"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate
                        ifEmpty="false">
                     <test
                           property="cdr-workbench.isFolder">
                     </test>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="org.eclipse.ui.views.properties.PropertySheet"
            id="cdr-workbench.Properties"
            name="Properties"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            class="unc.lib.cdr.workbench.views.MetsProjectNavigator"
            icon="icons/24px-Crystal_Clear_app_ark.png"
            id="cdr-workbench.projectview"
            name="Navigator"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="unc.lib.cdr.workbench.preferences.StagingPreferencePage"
            id="cdr_producer.preferences.PreferencePage"
            name="Staging Configuration">
      </page>
   </extension>
   <extension
         id="cdr_producer.projectNature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="unc.lib.cdr.workbench.project.MetsProjectNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.eclipse.ui.Basic"
            class="unc.lib.cdr.workbench.project.NewSipProjectWizard"
            descriptionImage="icons/48px-Crystal_Clear_app_ark.png"
            finalPerspective="cdr-workbench.AccessionPerspective"
            hasPages="true"
            icon="icons/48px-Crystal_Clear_filesystem_folder_tar.png"
            id="cdr-workbench.wizard.new.project"
            name="Workbench Project"
            project="true">
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="gov.loc.mets.DivType"
            class="unc.lib.cdr.workbench.arrange.DivAdapterFactory">
         <adapter
               type="org.eclipse.core.resources.IResource">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="*">
      </perspectiveExtension>
      <perspectiveExtension
            targetID="cdr-workbench.AccessionPerspective">
         <view
               closeable="false"
               id="cdr-workbench.projectview"
               minimized="false"
               moveable="false"
               ratio=".25"
               relationship="left"
               relative="org.eclipse.ui.editorss">
         </view>
         <view
               id="cdr-workbench.Properties"
               minimized="false"
               moveable="true"
               ratio=".8"
               relationship="bottom"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               visible="true">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="cdr-workbench.ArrangementPerspective">
         <view
               closeable="true"
               id="cdr-workbench.ProjectNavigator"
               minimized="false"
               moveable="true"
               ratio=".26f"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               standalone="false"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="unc.lib.cdr.workbench.rcp.AccessionPerspective"
            fixed="false"
            icon="icons/48px-Crystal_Clear_app_ark2.png"
            id="cdr-workbench.AccessionPerspective"
            name="Capture">
         <description>
            This perspective arranges the user interface to support media inventory, accession and deaccession.  It also supports overview of the original file structures and staging status.
         </description>
      </perspective>
      <perspective
            class="unc.lib.cdr.workbench.rcp.ArrangementPerspective"
            fixed="false"
            icon="icons/48px-Crystal_Clear_app_lists.png"
            id="cdr-workbench.ArrangementPerspective"
            name="Arrange">
         <description>
            The Arrange perspective configures the user interface to support description and arrangement of the collection or deposit structure.
         </description>
      </perspective>
      <perspective
            class="unc.lib.cdr.workbench.rcp.BatchMetadataPerspective"
            fixed="false"
            icon="icons/48px-Crystal_Clear_app_kexi.png"
            id="cdr-workbench.BatchMetadataPerspective"
            name="Metadata">
         <description>
            The Metadata perspective support batch metadata operations at the item level.  This is where you import original metadata sources in various formats, map the metadata records to digital objects and cross-walk metadata fields to standard schemas.
         </description>
      </perspective>
      <perspective
            class="unc.lib.cdr.workbench.rcp.MetsXmlPerspective"
            fixed="false"
            icon="icons/48px-Crystal_Clear_app_phppg.png"
            id="cdr-workbench.MetsPerspective"
            name="METS">
         <description>
            The METS perspective allows the user to browse the underlying METS document in which all standard workbench information is recorded for each project.
         </description>
      </perspective>
   </extension>
   <extension
         id="unc.lib.cdr.workbench.markers.OriginalFileSet"
         name="Original File Set"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="true">
      </persistent>
      <attribute
            name="type">
      </attribute>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="false"
            id="cdr-workbench.objectContributionDiv"
            objectClass="gov.loc.mets.DivType">
      </objectContribution>
   </extension>
   <extension
         id="unc.lib.cdr.workbench.markers.Captured"
         name="captured"
         point="org.eclipse.core.resources.markers">
      <attribute
            name="mets.Div.ID">
      </attribute>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         id="unc.lib.cdr.workbench.markers.Staged"
         name="staged"
         point="org.eclipse.core.resources.markers">
      <persistent
            value="true">
      </persistent>
      <attribute
            name="stage.uri">
      </attribute>
      <attribute
            name="mets.File.GroupIDAttr">
      </attribute>
   </extension>
   <extension
         id="unc.lib.cdr.workbench.builders.StageBuilder"
         name="Stage Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="false">
         <run
               class="unc.lib.cdr.workbench.staging.StageBuilder">
         </run>
      </builder>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            class="unc.lib.cdr.workbench.accession.OriginalsDecorator"
            id="cdr-workbench.decoratorCaptured"
            label="Captured File Decorators"
            lightweight="true"
            location="BOTTOM_RIGHT"
            state="true">
         <enablement>
            <or>
               <objectClass
                     name="org.eclipse.core.resources.IResource">
               </objectClass>
               <objectClass
                     name="gov.loc.mets.DivType">
               </objectClass>
            </or>
         </enablement>
      </decorator>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewer
            viewerId="cdr-workbench.projectview">
         <popupMenu
               allowsPlatformContributions="true"
               id="cdr-workbench.projectpopups">
         </popupMenu>
      </viewer>
      <viewerContentBinding
            viewerId="cdr-workbench.projectview">
         <includes>
            <contentExtension
                  pattern="cdr-workbench.projectContent">
            </contentExtension>
            <contentExtension
                  pattern="cdr-workbench.arrangementContent">
            </contentExtension>
            <contentExtension
                  pattern="cdr-workbench.originalsContent">
            </contentExtension>
            <contentExtension
                  pattern="cdr-workbench.crosswalksContent">
            </contentExtension>
            <contentExtension
                  pattern="workbench_plugin.stagedFileContent">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            contentProvider="unc.lib.cdr.workbench.views.MetsProjectContentProvider"
            id="cdr-workbench.projectContent"
            labelProvider="unc.lib.cdr.workbench.views.MetsProjectLabelProvider"
            name="Workbench Projects"
            priority="high">
         <triggerPoints>
            <or>
               <instanceof
                     value="unc.lib.cdr.workbench.views.WorkbenchWorkbenchRoot">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
            </or>
         </possibleChildren>
      </navigatorContent>
      <navigatorContent
            activeByDefault="true"
            contentProvider="unc.lib.cdr.workbench.arrange.ArrangementContentProvider"
            id="cdr-workbench.arrangementContent"
            labelProvider="unc.lib.cdr.workbench.arrange.ArrangementLabelProvider"
            name="Arranged Items">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
            </or>
         </triggerPoints>
         <dropAssistant
               class="unc.lib.cdr.workbench.arrange.ArrangementCommonDropAdapterAssistant"
               id="cdr-workbench.arrangementDropAssistant">
            <possibleDropTargets>
               <or>
                  <instanceof
                        value="gov.loc.mets.DivType">
                  </instanceof>
                  <instanceof
                        value="unc.lib.cdr.workbench.arrange.ArrangementProjectElement">
                  </instanceof>
               </or></possibleDropTargets>
         </dropAssistant>
         <possibleChildren>
            <or>
               <instanceof
                     value="gov.loc.mets.DivType">
               </instanceof>
               <instanceof
                     value="unc.lib.cdr.workbench.arrange.ArrangementProjectElement">
               </instanceof>
               <instanceof
                     value="org.eclipse.core.resources.IFile">
               </instanceof>
               <instanceof
                     value="org.eclipse.core.resources.IFolder">
               </instanceof>
               <instanceof
                     value="gov.loc.mets.MdSecType">
               </instanceof>
            </or></possibleChildren>
         <commonSorter
               class="unc.lib.cdr.workbench.arrange.ArrangementViewerSorter"
               id="cdr-workbench.arrangementViewerSorter">
            <parentExpression>
               <or>
                  <instanceof
                        value="unc.lib.cdr.workbench.arrange.ArrangementProjectElement">
                  </instanceof>
                  <instanceof
                        value="gov.loc.mets.DivType">
                  </instanceof>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            activeByDefault="true"
            contentProvider="unc.lib.cdr.workbench.accession.OriginalsContentProvider"
            id="cdr-workbench.originalsContent"
            labelProvider="org.eclipse.ui.model.WorkbenchLabelProvider"
            name="Original Folders">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
            </or>
         </triggerPoints>
      </navigatorContent>
      <navigatorContent
            activeByDefault="true"
            contentProvider="unc.lib.cdr.workbench.xwalk.CrosswalkContentProvider"
            id="cdr-workbench.crosswalksContent"
            labelProvider="unc.lib.cdr.workbench.xwalk.CrosswalkLabelProvider"
            name="Metadata Crosswalks">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IFile">
               </instanceof>
               <instanceof
                     value="unc.lib.cdr.workbench.xwalk.CrosswalksProjectElement">
               </instanceof>
               <instanceof
                     value="gov.loc.mets.MdSecType">
               </instanceof>
            </or>
         </possibleChildren>
      </navigatorContent>
      <navigatorContent
            activeByDefault="true"
            contentProvider="unc.lib.cdr.workbench.staging.StagedFileContentProvider"
            id="workbench_plugin.stagedFileContent"
            labelProvider="org.eclipse.ui.model.WorkbenchLabelProvider"
            name="Staged Files">
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IProject">
               </instanceof>
            </or>
         </triggerPoints>
      </navigatorContent>
   </extension>
   <extension
         id="unc.lib.cdr.workbench.builders.CrosswalksBuilder"
         name="Crosswalk Builder"
         point="org.eclipse.core.resources.builders">
      <builder>
         <run
