<?xml version="1.0" encoding="utf-8" ?>
<extensions>
	<page
		id="overview"
	>
		<hidden>
			<extension
				id="org.eclipse.ui.workbench" />
		</hidden>
		<group
			path="page-content/bottom-left"
			default="true"
		>
			<extension
				id="standards-workbench"
				importance="high" />
		</group>
		<group
			path="page-content/bottom-right"
			default="true"
		>
		</group>
		<group
			path="page-content/top-left"
		>
			<extension
				id="overview-workbench-id"
				importance="high" />
		</group>
		<group
			path="page-content/top-right"
		>
			<extension
				id="concepts-workbench"
				importance="high" />
		</group>
	</page>
	<page
		id="whatsnew"
	>
		<hidden>
			<extension
				id="org.eclipse.ui.workbench" />
			<extension
				id="org.eclipse.ui.migration" />
			<extension
				id="org.eclipse.ui.news" />
		</hidden>
		<group
			path="page-content/bottom-left"
			default="true"
		>
		</group>
		<group
			path="page-content/bottom-right"
			default="true"
		>
		</group>
		<group
			path="page-content/top-left"
		>
			<extension
				id="workbench-releasenotes"
				importance="high" />
		</group>
		<group
			path="page-content/top-right"
		>
			<extension
				id="githubReleases"
				importance="callout" />
		</group>
	</page>
	<page
		id="webresources"
	>
		<group
			path="page-content/bottom-left"
			default="true"
		>
		</group>
		<group
			path="page-content/bottom-right"
			default="true"
		>
		</group>
		<group
			path="page-content/top-left"
		>
			<extension
				id="workbench-webresources"
				importance="high" />
		</group>
		<group
			path="page-content/top-right"
		>
			<extension
				id="workbench-otherresources"
				importance="high" />
		</group>
	</page>
</extensions>