<?xml version="1.0" encoding="UTF-8"?>
<gmftool:ToolRegistry xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <palette
      title="crosswalkPalette">
    <tools
        xsi:type="gmftool:ToolGroup"
        title="Metadata Mapping"
        collapsible="true">
      <tools
          xsi:type="gmftool:CreationTool"
          title="XML Element"
          description="Add an Element"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="XML Attribute"
          description="Create an XML Attribute"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Input Connector"
          description="link to input"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Original Name Matcher"
          description="Create Original File/Folder Name Record Matcher"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Date Maker"
          description="Add a Date Recognizer"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Fixed Text"
          description="Add custom text to your crosswalk records"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Trim Spaces"
          description="Create Normalize Whitespace">
        <smallIcon
            xsi:type="gmftool:DefaultImage"/>
        <largeIcon
            xsi:type="gmftool:DefaultImage"/>
      </tools>
    </tools>
    <tools
        xsi:type="gmftool:ToolGroup"
        title="Dictionary Tools"
        collapsible="true">
      <tools
          xsi:type="gmftool:CreationTool"
          title="Metadata Block"
          description="Add a metadata block to the dictionary"/>
      <tools
          xsi:type="gmftool:CreationTool"
          title="Input Field"
          description="Add an input field to a metadata block"/>
    </tools>
  </palette>
</gmftool:ToolRegistry>
